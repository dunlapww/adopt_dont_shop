<h1>Pets!</h1>

<form action="/shelters/<%=@shelter.id%>/pets" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Pet Image:<p>
  <input type='text' name='pet[image]'/><br/>
  <p>Pet Name:<p>
  <input type='text' name='pet[name]'/><br/>
  <p>Pet Description:<p>
  <input type='text' name='pet[description]'/><br/>
  <p>Pet Age:<p>
  <input type='text' name='pet[age]'/><br/>
  <p>Pet Sex:<p>
  <input type='text' name='pet[sex]'/><br/>
  <br/>
  <input type='submit' value ='Create Pet'>
</form>